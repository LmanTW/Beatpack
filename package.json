{
  "name": "Beatpack",
  "version": "0.0.1",
  "main": "./Beatpack/Main.js",
  "scripts": {
    "start": "electron .",
    "build": "npx electron-forge package --platform=linux,darwin,win32"
  },
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
